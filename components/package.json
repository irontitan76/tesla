{
  "name": "@nexus/components",
  "version": "0.0.1",
  "babel": {
    "plugins": [],
    "presets": [
      [
        "@nx/react/babel",
        {
          "runtime": "automatic",
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:@nx/react",
      "../.eslintrc.json"
    ],
    "ignorePatterns": [
      "!**/*"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx",
          "*.js",
          "*.jsx"
        ],
        "rules": {}
      },
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {}
      },
      {
        "files": [
          "*.js",
          "*.jsx"
        ],
        "rules": {}
      }
    ]
  },
  "jest": {
    "coverageDirectory": "../coverage/components",
    "displayName": "components",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "preset": "../jest.preset.js",
    "transform": {
      "^.+\\.[tj]sx?$": [
        "babel-jest",
        {
          "presets": [
            "@nx/react/babel"
          ]
        }
      ],
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "@nx/react/plugins/jest"
    }
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/sheet": "^1.2.2",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fortawesome/sharp-light-svg-icons": "^6.4.2",
    "@mui/material": "^5.14.8",
    "@mui/x-data-grid-premium": "^6.14.0",
    "@supabase/supabase-js": "^2.33.2",
    "react": "^18.2.0",
    "react-cookie": "^6.1.1",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "typescript": "~5.1.3",
    "vite": "~4.3.9"
  }
}
