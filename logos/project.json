{
  "name": "logos",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "logos/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
          "commands": [
            "mkdir -p ../dist/logos/src",
            "cp -r src/* ../dist/logos/src",
            "cp -r package.json ../dist/logos/package.json"
          ],
          "cwd": "logos/"
      }
  },
    "publish": {
      "command": "node scripts/publish.mjs configs {args.ver} {args.tag}",
      "dependsOn": ["build"]
    }
  },
  "tags": []
}
